<%= render "navBar" %>

<div class="card-columns" >
<% if user_signed_in? %>
  <% @migrane.each do |migrane| %>
    <% if migrane.user_id == current_user.id %>
    <div class="card" style="background-color: #ffffe6">
      <div class="card-body">
        <h5 class="card-title"><%= link_to migrane.migrane_date.strftime("%d of %B, %Y"), migrane%></h5>
        <p class="card-text"><%= migrane.severity %></p>
        <p class="card-text"><%= migrane.notes %></p>
        </p>
      </div>    
    </div> 
     <% end %>
  <% end %> 
<% end %>
</div>

<%# if can? :crud @migrane %>
  <p> this is a test </p>
<%# end %>